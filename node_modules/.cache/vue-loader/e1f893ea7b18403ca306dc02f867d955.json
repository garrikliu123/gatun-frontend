\n        :name=\"item.orderId\"\n      >\n        <template slot=\"title\">\n          <div class=\"collapse-title\">\n            <div class=\"title-row\">\n              <i class=\"icon el-icon-postcard\"></i>\n              <span class=\"field\">Order ID:</span>\n              <span class=\"content\">{{item.orderId}}</span>\n              <el-divider direction=\"vertical\"></el-divider>\n            </div>\n\n            <div class=\"title-row\">\n              <i class=\"icon el-icon-date\"></i>\n              <span class=\"field\">Order Time:</span>\n              <span class=\"content\">{{new Date(item.createTime).toUTCString()}}</span>\n              <el-divider direction=\"vertical\"></el-divider>\n            </div>\n\n            <div class=\"title-row\">\n              <i class=\"icon el-icon-money\"></i>\n              <span class=\"field\">Total:</span>\n              <span\n                class=\"content\"\n                v-if=\"item.total >= 1000\"\n                v-format=\"'CAD $#,##0.00'\"\n              >\n                {{ item.total }}\n              </span>\n              <span\n                class=\"content\"\n                v-else\n                v-format=\"'CAD $##0.00'\"\n              >\n                {{ item.total }}\n              </span>\n            </div>\n          </div>\n        </template>\n        <div\n          class=\"product-item\"\n          v-for=\"(product, index) in item.orderDetail\"\n          :key=\"index\"\n        >\n          <div class=\"product-wrap\">\n            <el-image\n              style=\"width: 100px; height: 100px\"\n              :src=\"product.imageUrl\"\n              fit=\"contain\"\n            ></el-image>\n            <div class=\"text-group\">\n\n            </div>\n            <div class=\"right-group\">\n              <div\n                class=\"price\"\n                v-if=\"product.amount / 100 * product.quantity >= 1000\"\n                v-format=\"'$#,##0.00'\"\n              >\n                {{ product.amount / 100 * product.quantity }}\n              </div>\n              <div\n                class=\"price\"\n                v-else\n                v-format=\"'$##0.00'\"\n              >\n                {{ product.amount / 100 * product.quantity }}\n              </div>\n            </div>\n          </div>\n          <el-divider></el-divider>\n        </div>\n      </el-collapse-item>\n    </el-collapse>\n  </div>\n</template>\n<script>\nimport _ from \"lodash\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  data() {\n    return {\n      orderList: []\n    };\n  },\n  computed: {\n    ...mapGetters([\"orderHistory\"])\n  },\n\n  watch: {\n    orderHistory: {\n      immediate: true,\n      handler(list) {\n        this.orderList = _.cloneDeep(list);\n        this.orderList.forEach(order => {\n          let total = 0;\n          order.orderDetail.forEach(item => {\n            total += item.amount * item.quantity;\n          });\n          order.total = total / 100;\n        });\n        console.log(list);\n      }\n    }\n  }\n};\n</script>\n<style lang=\"less\" scoped>\n.ap-account-order {\n  .collapse-title {\n    display: flex;\n    .title-row {\n      display: flex;\n      align-items: center;\n    }\n    .icon {\n      font-size: 1.2em;\n      margin-right: 5px;\n    }\n\n    .field {\n      font-size: 1.2em;\n      font-weight: bold;\n      margin-right: 10px;\n    }\n\n    .content {\n      color: #697386;\n    }\n  }\n\n  .product-item {\n    .el-collapse-item__content {\n      background-color: #f5f7fa;\n    }\n\n    .product-wrap {\n      display: flex;\n    }\n\n    .text-group {\n      width: 400px;\n    }\n\n    .right-group {\n      .price {\n        font-size: 1.2em;\n        font-weight: bold;\n      }\n    }\n  }\n}\n</style>"]}]}