         v-for=\"item in matchedProduct.productImageList || []\"\n          :key=\"item.name\"\n        >\n          <div class=\"carousel_img_container\">\n            <img\n              class=\"carousel_img\"\n              :src=\"item.url\"\n            />\n          </div>\n        </el-carousel-item>\n      </el-carousel>\n      <div class=\"right-group\">\n        <div class=\"price\">CAD ${{matchedProduct.productPrice}}</div>\n        <el-button type=\"success\">成功按钮</el-button>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport { mapGetters } from \"vuex\";\nexport default {\n  props: {\n    productId: {\n      default: \"\"\n    }\n  },\n\n  computed: {\n    matchedProduct() {\n      const product = this.productList.find(product => {\n        if (product.productId === this.productId) {\n          product.productImageList = JSON.parse(product.productImages);\n          return true;\n        }\n      }) || {\n        success: false\n      };\n      return product;\n    },\n    ...mapGetters([\"productList\"])\n  },\n\n  watch: {\n    matchedProduct(product) {\n      if (product && product.success === false) {\n        this.$router.push(\"/\");\n      }\n    }\n  }\n};\n</script>\n<style lang=\"less\" scoped>\n.ap-product {\n  .title {\n    margin-bottom: 30px;\n  }\n\n  .product-show {\n    display: flex;\n    justify-content: space-between;\n\n    .ap-carousel {\n      width: 700px;\n      height: 500px;\n      margin-right: 20px;\n      box-shadow: 0px 2px 10px #eee;\n      border: 1px solid #eee;\n      border-radius: 4px;\n    }\n  }\n}\n</style>"]}]}