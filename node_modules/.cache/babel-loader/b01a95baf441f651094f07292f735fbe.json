IAAA,GAAA,cAAA,CAAA,IAAA,CAAA,GAAA;AACA,WAAA,KAAA,CAAA,iBAAA,CAAA,aAAA,GAAA,IAAA;AACA,WAAA,cAAA,GAAA;AACA,QAAA,YAAA,EAAA,EADA;AAEA,QAAA,YAAA,EAAA,mBAAA,CAAA,WAAA,CAAA,CAAA,EAAA;AAFA,OAAA;AAIA,KARA;AAUA,IAAA,kBAVA,8BAUA,QAVA,EAUA;AACA,WAAA,IAAA,GAAA,cAAA,CAAA,IAAA,CAAA,IAAA;AACA,WAAA,KAAA,CAAA,iBAAA,CAAA,aAAA,GAAA,IAAA;AACA,WAAA,cAAA,GAAA,QAAA;AACA,KAdA;AAgBA,IAAA,mBAhBA,+BAgBA,QAhBA,EAgBA;AAAA;;AACA,UAAA,UAAA,GAAA,QAAA,CAAA,UAAA;AACA,UAAA,YAAA,GAAA,QAAA,CAAA,YAAA;AAEA,WAAA,QAAA,mCAAA,YAAA,WAAA,eAAA,EAAA;AACA,QAAA,yBAAA,EAAA,IADA;AAEA,QAAA,iBAAA,EAAA,QAFA;AAGA,QAAA,gBAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,WAAA,CAAA,cAAA,CAAA,UAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,KAAA,CAAA,eAAA;;AACA,UAAA,KAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,SADA;AAEA,YAAA,OAAA,EAAA,gBAFA;AAGA,YAAA,SAAA,EAAA;AAHA,WAAA;AAKA,SAPA;AAQA,OAbA;AAcA;AAlCA,KAoCA,UAAA,CAAA,CAAA,iBAAA,CAAA,CApCA,CAfA;AAsDA,EAAA,UAAA,EAAA;AACA,6BAAA;AADA;AAtDA,CAAA","sourcesContent":["<template>\n  <div class=\"ap-manage-product\">\n    <!-- Add new category button 添加新分类按钮 -->\n    <div class=\"add-btn-container\">\n      <el-button type=\"primary\" @click=\"handleCategoryAdd\">ADD CATEGORY</el-button>\n    </div>\n\n    <!-- Category List Table 分类列表 -->\n    <el-table\n      class=\"product-table\"\n      max-height=\"650\"\n      :data=\"\n        categoryList.filter(\n          data =>\n            (!search || data.categoryName.toLowerCase().includes(search.toLowerCase())) &&\n            data.categoryType === categoryType\n        )\n      \"\n      style=\"width: 100%\"\n    >\n      <el-table-column\n        v-for=\"(config, index) in categoryTableConfig.rows\"\n        :key=\"index\"\n        :prop=\"config.key\"\n        :label=\"config.label\"\n        :width=\"config.width\"\n      >\n      </el-table-column>\n\n      <!-- Product Operation Column 商品操作列（修改，删除） -->\n      <el-table-column align=\"right\">\n        <template slot=\"header\" slot-scope=\"scope\">\n          <div class=\"header-control\">\n            <el-select class=\"ap-select\" v-model=\"categoryType\" placeholder=\"Select the type\">\n              <el-option\n                v-for=\"item in categoryTableConfig.typeOptions\"\n                :key=\"item.value\"\n                :label=\"item.label\"\n                :value=\"item.value\"\n              ></el-option>\n            </el-select>\n            <el-input v-model=\"search\" placeholder=\"Please enter keyword\" />\n            <span style=\"display: none\">{{ scope.$index }}</span>\n          </div>\n        </template>\n        <template slot-scope=\"scope\">\n          <el-button @click=\"handleCategoryEdit(scope.row)\">\n            Edit\n          </el-button>\n          <el-button type=\"danger\" @click=\"handleCateroyDelete(scope.row)\">\n            Delete\n          </el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <ap-addcategory-dialog\n      ref=\"addCategoryDialog\"\n      :mode=\"mode\"\n      :targetCategory=\"targetCategory\"\n    ></ap-addcategory-dialog>\n  </div>\n</template>\n<script>\nimport { mapGetters, mapActions } from \"vuex\";\nimport categoryApi from \"api/category\";\nimport categoryConfig from \"./config/category_config.js\";\nimport categoryTableConfig from \"./config/category_table_config.js\";\nimport AddCategoryDialog from \"./AddCategoryDialog\";\n\nexport default {\n  data() {\n    return {\n      mode: categoryConfig.MODE.NEW,\n      search: \"\",\n      categoryTableConfig,\n      categoryType: categoryTableConfig.typeOptions[0].value,\n      targetCategory: null\n    };\n  },\n\n  computed: {\n    ...mapGetters([\"categoryList\"])\n  },\n\n  methods: {\n    handleCategoryAdd() {\n      this.mode = categoryConfig.MODE.NEW;\n      this.$refs.addCategoryDialog.dialogVisible = true;\n      this.targetCategory = {\n        categoryName: \"\",\n        categoryType: categoryTableConfig.typeOptions[0].value\n      };\n    },\n\n    handleCategoryEdit(category) {\n      this.mode = categoryConfig.MODE.EDIT;\n      this.$refs.addCategoryDialog.dialogVisible = true;\n      this.targetCategory = category;\n    },\n\n    handleCateroyDelete(category) {\n      const categoryId = category.categoryId;\n      const categoryName = category.categoryName;\n\n      this.$confirm(`Do you want to delete \"${categoryName}\"? `, \"Delete Action\", {\n        distinguishCancelAndClose: true,\n        confirmButtonText: \"Delete\",\n        cancelButtonText: \"Cancel\"\n      }).then(() => {\n        categoryApi.deleteCategory(categoryId).then(() => {\n          this.getCategoryList();\n          this.$message({\n            type: \"success\",\n            message: \"Delete Success\",\n            showClose: true\n          });\n        });\n      });\n    },\n\n    ...mapActions([\"getCategoryList\"])\n  },\n\n  components: {\n    \"ap-addcategory-dialog\": AddCategoryDialog\n  }\n};\n</script>\n<style lang=\"less\" scoped>\n.ap-manage-product {\n  .add-btn-container {\n    width: 100%;\n    display: flex;\n  }\n\n  .product-table {\n    margin-top: 20px;\n    .header-control {\n      display: flex;\n    }\n\n    .ap-select {\n      margin-right: 20px;\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/views/Manage"}]}