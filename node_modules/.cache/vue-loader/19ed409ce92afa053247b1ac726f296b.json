\"onAddCartClick\"\n        ></ap-product-show>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport apProductShow from \"components/ProductShow\";\n\nexport default {\n  props: {\n    title: {\n      type: String,\n      default: \"\",\n    },\n    productList: {\n      type: Array,\n      default: () => {\n        return [];\n      },\n    },\n  },\n\n  methods: {\n    onProductClick(productId) {\n      this.$router.push(\"/product/\" + productId);\n    },\n\n    onAddCartClick() {\n      console.log(\"Add Cart\");\n    },\n  },\n\n  components: {\n    \"ap-product-show\": apProductShow,\n  },\n};\n</script>\n<style lang=\"less\" scoped>\n.product-list-container {\n  .product-list-title {\n    margin-bottom: 30px;\n  }\n\n  .product-list {\n    display: flex;\n    flex-direction: row;\n    flex-wrap: wrap;\n  }\n\n  .product-show-container {\n    flex: 0 0 25%;\n  }\n\n  .product-show {\n    cursor: pointer;\n    margin: 0 10px 10px 0px;\n  }\n}\n</style>\n"]}]}