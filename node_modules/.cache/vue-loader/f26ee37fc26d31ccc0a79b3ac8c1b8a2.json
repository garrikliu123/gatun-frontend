iv class=\"ap-manage-product\">\n    <!-- Add new product button 添加新产品按钮 -->\n    <div class=\"add-btn-container\">\n      <el-button\n        type=\"primary\"\n        @click=\"handleProductAdd\"\n      >ADD PRODUCT</el-button>\n    </div>\n\n    <!-- Product List Table 商品列表 -->\n    <el-table\n      class=\"product-table\"\n      :data=\"\n        productList.filter(\n          data => !search || data.product_name.toLowerCase().includes(search.toLowerCase())\n        )\n      \"\n      style=\"width: 100%\"\n    >\n      <el-table-column\n        v-for=\"(config, index) in productTableConfig.rows\"\n        :key=\"index\"\n        :prop=\"config.key\"\n        :label=\"config.label\"\n        :width=\"config.width\"\n      >\n      </el-table-column>\n\n      <!-- Product Operation Column 商品操作列（修改，删除） -->\n      <el-table-column align=\"right\">\n        <template\n          slot=\"header\"\n          slot-scope=\"scope\"\n        >\n          <el-input\n            v-model=\"search\"\n            placeholder=\"Please enter keyword\"\n          />\n          <span style=\"display: none\">{{ scope.$index }}</span>\n        </template>\n        <template slot-scope=\"scope\">\n          <el-button @click=\"handleProductEdit(scope.$index, scope.row)\">\n            Edit\n          </el-button>\n          <el-button\n            type=\"danger\"\n            @click=\"handleProductDelete(scope.$index, scope.row)\"\n          >\n            Delete\n          </el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <ap-addproduct-dialog ref=\"addProductDialog\"></ap-addproduct-dialog>\n  </div>\n</template>\n<script>\nimport { mapGetters } from \"vuex\";\nimport productTableConfig from \"./config/product_table_config.js\";\nimport AddProductDialog from \"./AddProductDialog\";\n// import productApi from \"api/product\";\n\nexport default {\n  data() {\n    return {\n      search: \"\",\n      productTableConfig\n    };\n  },\n\n  computed: {\n    ...mapGetters([\"productList\"])\n  },\n\n  methods: {\n    handleProductAdd() {\n      this.$refs.addProductDialog.dialogVisible = true;\n    },\n    handleProductEdit() {},\n    handleProductDelete() {}\n  },\n\n  components: {\n    \"ap-addproduct-dialog\": AddProductDialog\n  }\n};\n</script>\n<style lang=\"less\" scoped>\n.ap-manage-product {\n  .add-btn-container {\n    width: 100%;\n    display: flex;\n  }\n\n  .product-table {\n    margin-top: 20px;\n  }\n}\n</style>\n"]}]}