OAAA,CAAA,YAAA,CAAA;AACA,UAAA,OAAA,CAAA,iBAAA,GAAA,IAAA,CAAA,KAAA,CAAA,OAAA,CAAA,cAAA,CAAA;AACA,iBAAA,IAAA;AACA;AACA,OAPA,KAOA;AACA,QAAA,OAAA,EAAA;AADA,OAPA;AAUA,aAAA,OAAA;AACA,KAbA;AAcA,IAAA,SAdA,uBAcA;AACA,UAAA,IAAA,GAAA,EAAA;;AACA,UAAA,KAAA,cAAA,CAAA,gBAAA,EAAA;AACA,QAAA,IAAA,GAAA,KAAA,cAAA,CAAA,gBAAA,CAAA,GAAA,CAAA,UAAA,KAAA,EAAA;AACA,iBAAA,KAAA,CAAA,GAAA;AACA,SAFA,CAAA;AAGA;;AAEA,aAAA,IAAA;AACA,KAvBA;AAwBA,IAAA,cAxBA,4BAwBA;AACA,UAAA,IAAA,GAAA,EAAA;AACA,UAAA,iBAAA,GAAA,KAAA,cAAA,CAAA,iBAAA;;AACA,UAAA,iBAAA,IAAA,iBAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,QAAA,iBAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,UAAA,IAAA,CAAA,IAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,KAAA,EAAA;AAFA,WAAA;AAIA,SALA;AAMA;;AACA,aAAA,IAAA;AACA;AApCA,KAqCA,UAAA,CAAA,CAAA,aAAA,EAAA,cAAA,EAAA,aAAA,CAAA,CArCA,CAdA;AAsDA,EAAA,KAAA,EAAA;AACA,IAAA,cADA,0BACA,OADA,EACA;AACA,UAAA,OAAA,IAAA,OAAA,CAAA,OAAA,KAAA,KAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,KALA;AAMA,IAAA,cAAA,EAAA;AACA,MAAA,SAAA,EAAA,IADA;AAEA,MAAA,IAAA,EAAA,IAFA;AAGA,MAAA,OAHA,mBAGA,IAHA,EAGA;AACA,YAAA,IAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,eAAA,YAAA,GAAA,IAAA,CAAA,CAAA,CAAA,CAAA,KAAA;AACA;AACA;AAPA;AANA,GAtDA;AAuEA,EAAA,OAAA;AACA,IAAA,UADA,wBACA;AACA,WAAA,gBAAA,CAAA;AACA,QAAA,SAAA,EAAA,KAAA,cAAA,CAAA,SADA;AAEA,QAAA,MAAA,EAAA,KAAA,KAFA;AAGA,QAAA,MAAA,EAAA,KAAA;AAHA,OAAA;AAMA,WAAA,QAAA,CAAA;AACA,QAAA,IAAA,EAAA,SADA;AAEA,QAAA,OAAA,EAAA,cAFA;AAGA,QAAA,SAAA,EAAA;AAHA,OAAA;AAKA;AAbA,KAeA,UAAA,CAAA,CAAA,kBAAA,CAAA,CAfA,CAvEA;AAyFA,EAAA,UAAA,EAAA;AACA,sBAAA;AADA;AAzFA,CAAA","sourcesContent":["<template>\n  <div class=\"ap-product\">\n    <div class=\"product-show\">\n      <!-- Carousel 轮播图 -->\n      <div class=\"title\" v-if=\"isPhoneSize\">\n        {{ matchedProduct.productName }}\n      </div>\n      <el-carousel class=\"ap-carousel\" height=\"500px\">\n        <el-carousel-item v-for=\"item in matchedProduct.productImageList || []\" :key=\"item.name\">\n          <el-image class=\"carousel_img\" fit=\"contain\" :src=\"item.url\"></el-image>\n        </el-carousel-item>\n      </el-carousel>\n      <div class=\"right-group\">\n        <div class=\"title\" v-if=\"!isPhoneSize\">\n          {{ matchedProduct.productName }}\n        </div>\n        <el-divider content-position=\"right\"\n          ><i class=\"ap-nav-icon el-icon-shopping-cart-2\" style=\"font-size: 1.2em;\"></i\n        ></el-divider>\n        <div class=\"price\" v-if=\"matchedProduct.productPrice >= 1000\" v-format=\"'CAD $#,##0.00'\">\n          {{ matchedProduct.productPrice }}\n        </div>\n        <div class=\"price\" v-else v-format=\"'CAD $##0.00'\">\n          {{ matchedProduct.productPrice }}\n        </div>\n        <el-select\n          v-if=\"productOptions.length > 0\"\n          class=\"option-select\"\n          v-model=\"optionChoose\"\n          placeholder=\"Please select an option\"\n          size=\"small\"\n        >\n          <el-option\n            v-for=\"item in productOptions\"\n            :key=\"item.value\"\n            :label=\"item.label\"\n            :value=\"item.value\"\n          >\n          </el-option>\n        </el-select>\n        <el-input-number class=\"count\" v-model=\"count\" :min=\"1\" :max=\"100\"></el-input-number>\n        <el-button type=\"success\" class=\"button\" @click=\"onAddClick\">ADD TO CART</el-button>\n      </div>\n    </div>\n    <ap-tab-display class=\"ap-tab-display\" :product=\"matchedProduct\"></ap-tab-display>\n  </div>\n</template>\n<script>\nimport { mapGetters, mapActions } from \"vuex\";\nimport TabDisplay from \"./TabDisplay\";\nexport default {\n  props: {\n    productId: {\n      default: \"\"\n    }\n  },\n\n  data() {\n    return {\n      count: 1,\n      optionChoose: \"\"\n    };\n  },\n\n  computed: {\n    matchedProduct() {\n      const product = this.productList.find(product => {\n        if (product.productId === this.productId) {\n          product.productImageList = JSON.parse(product.productImages);\n          product.productFileList = JSON.parse(product.productFiles);\n          product.productOptionList = JSON.parse(product.productOptions);\n          return true;\n        }\n      }) || {\n        success: false\n      };\n      return product;\n    },\n    imageList() {\n      let list = [];\n      if (this.matchedProduct.productImageList) {\n        list = this.matchedProduct.productImageList.map(image => {\n          return image.url;\n        });\n      }\n\n      return list;\n    },\n    productOptions() {\n      const list = [];\n      const productOptionList = this.matchedProduct.productOptionList;\n      if (productOptionList && productOptionList.length > 0) {\n        productOptionList.forEach(item => {\n          list.push({\n            label: item,\n            value: item\n          });\n        });\n      }\n      return list;\n    },\n    ...mapGetters([\"productList\", \"shoppingCart\", \"isPhoneSize\"])\n  },\n\n  watch: {\n    matchedProduct(product) {\n      if (product && product.success === false) {\n        this.$router.push(\"/\");\n      }\n    },\n    productOptions: {\n      immediate: true,\n      deep: true,\n      handler(list) {\n        if (list.length > 0) {\n          this.optionChoose = list[0].value;\n        }\n      }\n    }\n  },\n\n  methods: {\n    onAddClick() {\n      this.addProudctToCart({\n        productId: this.matchedProduct.productId,\n        amount: this.count,\n        option: this.optionChoose\n      });\n\n      this.$message({\n        type: \"success\",\n        message: \"Add Success!\",\n        showClose: true\n      });\n    },\n\n    ...mapActions([\"addProudctToCart\"])\n  },\n\n  components: {\n    \"ap-tab-display\": TabDisplay\n  }\n};\n</script>\n<style lang=\"less\">\n.ap-product {\n  .el-carousel__item {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n  }\n\n  .el-carousel {\n    .el-carousel__indicators {\n      width: 100% !important;\n    }\n\n    &:hover {\n      .el-carousel__indicators {\n        background-color: rgba(31, 45, 61, 0.1);\n        transition: background 500ms ease-in-out;\n      }\n    }\n  }\n\n  .product-show {\n    width: 100%;\n    display: flex;\n    margin: 30px 0px;\n\n    .ap-carousel {\n      flex: 0 0 700px;\n      height: 500px;\n      margin-right: 60px;\n      box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.12), 0 0 6px 0 rgba(0, 0, 0, 0.04);\n      border-radius: 4px;\n\n      .el-image {\n        height: 100% !important;\n        width: 100% !important;\n      }\n    }\n\n    .right-group {\n      flex: 1 1 auto;\n      display: flex;\n      flex-direction: column;\n      align-items: flex-start;\n    }\n\n    .title {\n      margin-bottom: 50px;\n      font-size: 1.5em;\n      word-break: break-all;\n      text-align: left;\n      font-weight: bold;\n    }\n\n    .price {\n      font-size: 1.5em;\n      font-weight: bold;\n      margin-bottom: 40px;\n    }\n\n    .button {\n      font-size: 1.8em;\n    }\n\n    .count {\n      margin-bottom: 30px;\n    }\n\n    .option-select {\n      margin-bottom: 20px;\n    }\n  }\n\n  .ap-tab-display {\n    margin-bottom: 30px;\n  }\n}\n\n.is_phone .product-show {\n  display: flex;\n  flex-direction: column !important;\n  align-items: center;\n  .title {\n    margin-bottom: 20px;\n  }\n  .ap-carousel {\n    flex: 1 1 auto;\n    width: 100%;\n    height: 300px !important;\n    margin: 0;\n\n    .el-carousel__container {\n      height: 100% !important;\n    }\n  }\n\n  .right-group {\n    width: 100%;\n    align-items: center;\n  }\n}\n</style>\n"],"sourceRoot":"src/views/Product"}]}