,IAAA,KAAA,IAAA,CAAA,QAAA,KAAA,SAAA,EAAA;AACA,iBAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,SAJA,MAIA,IAAA,IAAA,IAAA,UAAA,EAAA;AACA,cAAA,CAAA,KAAA,IAAA,EAAA;AACA,iBAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA;AACA;AAZA;AADA,GAdA;AA+BA,EAAA,OAAA;AACA,IAAA,eADA,2BACA,GADA,EACA;AACA,UAAA,SAAA,GAAA,KAAA,OAAA,CAAA,GAAA,CAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,SAAA,CAAA,IAAA;AACA,KAJA;AAMA,IAAA,WANA,uBAMA,WANA,EAMA,EANA,EAMA;AACA,UAAA,MAAA,GAAA,EAAA;AACA,WAAA,WAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,QAAA,GAAA,IAAA,CAAA,WAAA,CACA,WADA,GAEA,OAFA,CAEA,WAAA,CAAA,WAAA,EAFA,CAAA;;AAGA,YAAA,QAAA,IAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA,CAAA;AACA,YAAA,KAAA,EAAA,IAAA,CAAA,WADA;AAEA,YAAA,EAAA,EAAA,IAAA,CAAA,SAFA;AAGA,YAAA,QAAA,EAAA;AAHA,WAAA;AAKA;AACA,OAXA;AAaA,MAAA,MAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,YAAA,CAAA,CAAA,QAAA,GAAA,CAAA,CAAA,QAAA,EAAA;AACA,iBAAA,CAAA,CAAA;AACA,SAFA,MAEA,IAAA,CAAA,CAAA,QAAA,IAAA,CAAA,CAAA,QAAA,EAAA;AACA,iBAAA,CAAA;AACA,SAFA,MAEA;AACA,iBAAA,CAAA;AACA;AACA,OARA;AAUA,MAAA,EAAA,CAAA,MAAA,CAAA;AACA,KAhCA;AAkCA,IAAA,aAlCA,2BAkCA;AACA,UAAA,KAAA,MAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,aAAA,KAAA,MAAA;AACA,OAFA,MAEA;AACA,aAAA,QAAA,CAAA;AACA,UAAA,OAAA,EAAA,4BADA;AAEA,UAAA,IAAA,EAAA,SAFA;AAGA,UAAA,SAAA,EAAA;AAHA,SAAA;AAKA;AACA,KA5CA;AA8CA,IAAA,WA9CA,uBA8CA,IA9CA,EA8CA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,WAAA,KAAA,CAAA,WAAA,CAAA,aAAA,GAAA,IAAA;AACA,KAjDA;AAmDA,IAAA,aAnDA,2BAmDA;AACA,WAAA,MAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,MAAA;AACA;AAtDA,KAwDA,UAAA,CAAA,CAAA,QAAA,CAAA,CAxDA,CA/BA;AA0FA,EAAA,UAAA,EAAA;AACA,gBAAA,WADA;AAEA,eAAA;AAFA;AA1FA,CAAA","sourcesContent":["<template>\n  <div class=\"ap-header\">\n    <el-menu\n      class=\"ap-nav\"\n      mode=\"horizontal\"\n      :default-active=\"activeIndex\"\n      @select=\"handleNavSelect\"\n    >\n      <div class=\"ap-nav-logo__container\">\n        <router-link to=\"/\">\n          <h1 class=\"nav-title\">GATUN</h1>\n        </router-link>\n        <!-- <img class=\"ap-nav-logo\" src=\"~common/images/logo.png\" alt=\"APERA\" /> -->\n      </div>\n      <template v-for=\"(item, index) in navList\">\n        <el-menu-item\n          v-if=\"item.onlyManager ? user && user.userType == 'manager' : true\"\n          :key=\"index\"\n          :index=\"index.toString()\"\n        >\n          {{ item.title }}\n        </el-menu-item>\n      </template>\n\n      <el-autocomplete\n        class=\"ap-nav-search\"\n        placeholder=\"Please enter keyword\"\n        v-model=\"search\"\n        :fetch-suggestions=\"querySearch\"\n        :trigger-on-focus=\"false\"\n      >\n        <el-button\n          slot=\"append\"\n          icon=\"el-icon-search\"\n          @click=\"onSearchClick\"\n        ></el-button>\n      </el-autocomplete>\n      <div class=\"ap-nav-icon__group\">\n        <el-dropdown class=\"user\">\n          <span class=\"el-dropdown-link\">\n            <i class=\"ap-nav-icon el-icon-user-solid\"></i>\n          </span>\n          <el-dropdown-menu\n            slot=\"dropdown\"\n            v-if=\"!user\"\n          >\n            <el-dropdown-item @click.native=\"onUserClick('login')\">Log In</el-dropdown-item>\n            <el-dropdown-item @click.native=\"onUserClick('register')\">Register</el-dropdown-item>\n          </el-dropdown-menu>\n          <el-dropdown-menu\n            slot=\"dropdown\"\n            v-else\n          >\n            <el-dropdown-item @click.native=\"$router.push('account')\">My Account</el-dropdown-item>\n            <el-dropdown-item @click.native=\"onLogoutClick()\">Log Out</el-dropdown-item>\n          </el-dropdown-menu>\n        </el-dropdown>\n        <el-popover\n          placement=\"bottom-end\"\n          width=\"500\"\n          trigger=\"click\"\n        >\n          <ap-cart></ap-cart>\n          <i\n            slot=\"reference\"\n            class=\"ap-nav-icon el-icon-shopping-cart-2\"\n          ></i>\n        </el-popover>\n      </div>\n    </el-menu>\n    <ap-login\n      ref=\"loginDialog\"\n      :mode=\"loginMode\"\n    ></ap-login>\n  </div>\n</template>\n<script>\nimport { mapGetters, mapActions } from \"vuex\";\nimport headerNavConfig from \"common/config/header_nav_config\";\nimport LoginDialog from \"views/Login\";\nimport Cart from \"components/Cart\";\nexport default {\n  data() {\n    return {\n      loginMode: \"login\",\n      search: \"\",\n      navList: headerNavConfig,\n      activeIndex: \"\"\n    };\n  },\n\n  computed: {\n    ...mapGetters([\"productList\", \"user\"])\n  },\n\n  watch: {\n    \"$route.fullPath\": {\n      immediate: true,\n      handler(path) {\n        if (path == \"/manage\") {\n          if (!this.user || this.user.userType !== \"manager\") {\n            this.$router.push(\"/\");\n          }\n        } else if (path == \"/account\") {\n          if (!this.user) {\n            this.$router.push(\"/\");\n          }\n        }\n      }\n    }\n  },\n\n  methods: {\n    handleNavSelect(key) {\n      const targetNav = this.navList[key];\n      this.$router.push(targetNav.link);\n    },\n\n    querySearch(queryString, cb) {\n      const result = [];\n      this.productList.forEach(item => {\n        const priority = item.productName\n          .toLowerCase()\n          .indexOf(queryString.toLowerCase());\n        if (priority >= 0) {\n          result.push({\n            value: item.productName,\n            id: item.productId,\n            priority\n          });\n        }\n      });\n\n      result.sort((a, b) => {\n        if (a.priority < b.priority) {\n          return -1;\n        } else if (a.priority == b.priority) {\n          return 0;\n        } else {\n          return 1;\n        }\n      });\n\n      cb(result);\n    },\n\n    onSearchClick() {\n      if (this.search) {\n        this.$router.push(\"/search/\" + this.search);\n      } else {\n        this.$message({\n          message: \"Please enter some keyword!\",\n          type: \"warning\",\n          showClose: true\n        });\n      }\n    },\n\n    onUserClick(type) {\n      this.loginMode = type;\n      this.$refs.loginDialog.dialogVisible = true;\n    },\n\n    onLogoutClick() {\n      this.logout();\n      this.$router.push(\"home\");\n    },\n\n    ...mapActions([\"logout\"])\n  },\n\n  components: {\n    \"ap-login\": LoginDialog,\n    \"ap-cart\": Cart\n  }\n};\n</script>\n<style lang=\"less\">\n.ap-header {\n  user-select: none;\n  outline: none;\n  .ap-nav-logo__container {\n    flex: 1 1 auto;\n    display: flex;\n    justify-content: flex-start;\n\n    a {\n      text-decoration: none;\n    }\n\n    .nav-title {\n      margin: 0;\n      text-align: left;\n      color: #2c3e50;\n    }\n  }\n  .ap-nav-logo {\n    width: 200px;\n  }\n\n  .ap-nav-icon__group {\n    flex: 0 0 auto;\n    .user {\n      margin-right: 10px;\n    }\n  }\n\n  .ap-nav-icon {\n    font-size: 1.5em;\n    margin-right: 20px;\n    cursor: pointer;\n    &:last-of-type {\n      margin-right: 0;\n    }\n  }\n\n  .ap-nav-search {\n    flex: 1 1 auto;\n    max-width: 250px;\n    margin: 0 20px;\n  }\n\n  .ap-nav {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    padding: 0px 10px;\n  }\n\n  .el-dropdown {\n    color: #2c3e50;\n    font-size: 16px;\n  }\n}\n</style>\n"],"sourceRoot":"src/components/Header"}]}