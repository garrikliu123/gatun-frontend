on 添加新产品按钮 -->\n    <div class=\"add-btn-container\">\n      <el-button type=\"primary\" @select=\"handleProductAdd\">ADD PRODUCT</el-button>\n    </div>\n\n    <!-- Product List Table 商品列表 -->\n    <el-table\n      class=\"product-table\"\n      :data=\"\n        productTableData.filter(\n          data => !search || data.product_name.toLowerCase().includes(search.toLowerCase())\n        )\n      \"\n      style=\"width: 100%\"\n    >\n      <el-table-column\n        v-for=\"(config, index) in productTableConfig.rows\"\n        :key=\"index\"\n        :prop=\"config.key\"\n        :label=\"config.label\"\n        :width=\"config.width\"\n      >\n      </el-table-column>\n\n      <!-- Product Operation Column 商品操作列（修改，删除） -->\n      <el-table-column align=\"right\">\n        <template slot=\"header\">\n          <el-input v-model=\"search\" placeholder=\"Please enter keyword\" />\n        </template>\n        <template slot-scope=\"scope\">\n          <el-button @click=\"handleProductEdit(scope.$index, scope.row)\">\n            Edit\n          </el-button>\n          <el-button type=\"danger\" @click=\"handleProductDelete(scope.$index, scope.row)\">\n            Delete\n          </el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n  </div>\n</template>\n<script>\nimport productTableConfig from \"common/config/product_table_config.js\";\n\nimport productApi from \"api/product\";\n\nexport default {\n  data() {\n    return {\n      search: \"\",\n      productTableConfig,\n      productTableData: []\n    };\n  },\n\n  mounted() {\n    this.handleProductGetAll();\n  },\n\n  methods: {\n    handleProductGetAll() {\n      productApi.getProductList().then(res => {\n        res.data.forEach(item => {\n          item.productPrice = \"$\" + item.productPrice;\n        });\n\n        this.productTableData = res.data;\n      });\n    },\n    handleProductAdd() {},\n    handleProductEdit() {},\n    handleProductDelete() {}\n  }\n};\n</script>\n<style lang=\"less\" scoped>\n.ap-manage-product {\n  .add-btn-container {\n    width: 100%;\n    display: flex;\n  }\n\n  .product-table {\n    margin-top: 20px;\n  }\n}\n</style>\n"],"sourceRoot":"src/views/Manage"}]}